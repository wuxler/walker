# Default
*            text=auto

# Golang
# ============
*.go -text diff=golang

# Lua
# ============
*.lua   text

# HTML
# =============
*.html  text diff=html
*.css   text diff=css

# C/C++
# =============
*.c     text diff=c
*.cc    text diff=cpp
*.cxx   text diff=cpp
*.cpp   text diff=cpp
*.c++   text diff=cpp
*.hpp   text diff=cpp
*.h     text diff=c
*.h++   text diff=cpp
*.hh    text diff=cpp
# Compiled Object files
*.slo   binary
*.lo    binary
*.o     binary
*.obj   binary
# Precompiled Headers
*.gch   binary
*.pch   binary
# Compiled Dynamic libraries
*.so    binary
*.dylib binary
*.dll   binary
# Compiled Static libraries
*.lai   binary
*.la    binary
*.a     binary
*.lib   binary
# Executables
*.exe   binary
*.out   binary
*.app   binary

# VisualStudio
# =============
*.sln        text eol=crlf
*.csproj     text eol=crlf
*.vbproj     text eol=crlf
*.vcxproj    text eol=crlf
*.vcproj     text eol=crlf
*.dbproj     text eol=crlf
*.fsproj     text eol=crlf
*.lsproj     text eol=crlf
*.wixproj    text eol=crlf
*.modelproj  text eol=crlf
*.sqlproj    text eol=crlf
*.wwaproj    text eol=crlf
*.xproj      text eol=crlf
*.props      text eol=crlf
*.filters    text eol=crlf
*.vcxitems   text eol=crlf